package Items;

import Main.Game;

public class Item () {
    
    protected int quantity, item;
    
    public Item(int quantity, int item) {
		this.quantity = this.quantity;
		this.item = item;
	}

    public void useItem(int )

	public int getItemType() {
		return item;
	}

	public int getxDrawOffset() {
		return xDrawOffset;
	}

	public int getyDrawOffset() {
		return yDrawOffset;
	}

	
}
